{
    "name": "node-js-jwt-auth-mongodb",
    "version": "1.0.0",
    "description": "Node.js + MongoDB: JWT Authentication & Authorization",
    "main": "server.js",
    "scripts": {
        "build": "cd ../react-app && npm run build",
        "install-client": "cd ../react-app && npm install",
        "heroku-postbuild": "npm run install-client && npm run build",
        "server": "nodemon server.js",
        "develop": "concurrently --kill-others-on-fail \"npm run server\" \"npm run start --prefix --prefix \"",
        "start": "concurrently --kill-others-on-fail \"npm run server\" \"cd ../react-app && npm run start  --prefix \""
    },
    "keywords": [
        "node.js",
        "express",
        "jwt",
        "authentication",
        "mongodb"
    ],
    "author": "bezkoder",
    "license": "ISC",
    "dependencies": {
        "@google-cloud/dialogflow": "^3.4.0",
        "action": "0.0.1",
        "actions-on-google": "^2.13.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "cheerio": "^1.0.0-rc.10",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dialogflow": "^1.2.0",
        "dialogflow-fulfillment": "^0.6.1",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.7",
        "express": "^4.17.1",
        "express-fileupload": "^1.3.1",
        "google": "^2.1.0",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^5.9.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.4",
        "nodemailer": "^6.4.17",
        "nodeman": "^1.1.2",
        "on": "^1.3.0",
        "request": "^2.88.2",
        "stripe": "^8.222.0",
        "superagent": "^7.1.3",
        "uuid": "^8.3.2"
    }
}
